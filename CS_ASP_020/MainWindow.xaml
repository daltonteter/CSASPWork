        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CS_ASP_020"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">
    <Grid>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="166" Margin="283,242,0,0" VerticalAlignment="Top" Width="673"/>

        <Label x:Name="labelMain" Content="Loan Form" HorizontalAlignment="Left" Margin="78,54,0,0" VerticalAlignment="Top" FontSize="24"/>
