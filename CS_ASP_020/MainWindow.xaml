        <TextBox x:Name="textBoxSalary" HorizontalAlignment="Left" Height="42" TextWrapping="Wrap" VerticalAlignment="Top" Width="174" Margin="173,435,0,0" FontSize="24"/>
        <Label x:Name="labelResult1" Content="Result Label" HorizontalAlignment="Left" Margin="283,234,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Button x:Name="buttonAction" Content="Push Me" HorizontalAlignment="Left" Margin="498,168,0,0" VerticalAlignment="Top" Width="106" FontSize="24" Height="41" Click="buttonAction_Click"/>
        <Label x:Name="labelSS" Content="Social Security Number" HorizontalAlignment="Left" Margin="385,448,0,0" VerticalAlignment="Top" FontSize="24"/>
        <TextBox x:Name="textBoxSS" HorizontalAlignment="Left" Height="38" Margin="670,448,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240" FontSize="24"/>
        <Label x:Name="labelResult2" Content="Result Label" HorizontalAlignment="Left" Margin="283,267,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label x:Name="labelResult3" Content="Result Label" HorizontalAlignment="Left" Margin="283,301,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label x:Name="labelResult4" Content="Result Label" HorizontalAlignment="Left" Margin="283,335,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label x:Name="labelResult5" Content="Result Label" HorizontalAlignment="Left" Margin="283,366,0,0" VerticalAlignment="Top" FontSize="24"/>

    </Grid>
</Window>
